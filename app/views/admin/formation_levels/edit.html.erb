<h1>Editing formation_level</h1>

<% form_for([:admin, @formation_level]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', [:admin, @formation_level] %> |
<%= link_to 'Back', admin_formation_levels_path %>
