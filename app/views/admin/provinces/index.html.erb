<h1>Listing provinces</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% for province in @provinces %>
  <tr>
    <td><%=h province.name %></td>
    <td><%= link_to 'Show', [:admin, province] %></td>
    <td><%= link_to 'Edit', edit_admin_province_path(province) %></td>
    <td><%= link_to 'Destroy', [:admin, province], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New province', new_admin_province_path %>
