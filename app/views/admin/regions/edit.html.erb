<h1>Editing region</h1>

<% form_for([:admin, @region]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', admin_region_path(@region) %> |
<%= link_to 'Back', admin_regions_path %>
